{
  "name": "@pinhask/notification",
  "version": "1.0.0",
  "description": "Reactive React Chat App",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "dev": "ts-node .",
    "build": "npm run lint && npm run tsc",
    "build:run": "npm run lint && npm run tsc && npm run dev",
    "app:build": "cd app && npm run bundle",
    "app:dev": "cd app && npm run dev",
    "install:all": "concurrently -m 1 \"npm i \" \"cd app && npm i \"",
    "run:all": "concurrently \"npm run build:run\" \"npm run app:dev\"",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "pinhas keizman",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.6.0",
    "bcrypt": "^5.0.0",
    "bootleg": "^0.9.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "highland": "^2.13.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "primus": "^8.0.1",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/passport": "^1.0.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/primus": "^7.3.3",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "chai": "^4.3.0",
    "concurrently": "^5.3.0",
    "mocha": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
